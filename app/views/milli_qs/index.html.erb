<p id="notice"><%= notice %></p>

<h1>Milli Qs</h1>

<table>
  <thead>
    <tr>
      <th>Typewater</th>
      <th>Volumeone</th>
      <th>Volumetwo</th>
      <th>More</th>
      <th>Debit</th>
      <th>Slotdate</th>
      <th>Slottime</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @milli_qs.each do |milli_q| %>
      <tr>
        <td><%= milli_q.typewater %></td>
        <td><%= milli_q.volumeone %></td>
        <td><%= milli_q.volumetwo %></td>
        <td><%= milli_q.more %></td>
        <td><%= milli_q.debit %></td>
        <td><%= milli_q.slotdate %></td>
        <td><%= milli_q.slottime %></td>
        <td><%= milli_q.status %></td>
        <td><%= link_to 'Show', milli_q %></td>
        <td><%= link_to 'Edit', edit_milli_q_path(milli_q) %></td>
        <td><%= link_to 'Destroy', milli_q, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Milli Q', new_milli_q_path %>
