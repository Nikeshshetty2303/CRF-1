<p id="notice"><%= notice %></p>

<h1>Probe Sonicators</h1>

<table>
  <thead>
    <tr>
      <th>Sample</th>
      <th>Size</th>
      <th>Amplitude</th>
      <th>Volume</th>
      <th>Viscosity</th>
      <th>More</th>
      <th>Debit</th>
      <th>Slotdate</th>
      <th>Slottime</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @probe_sonicators.each do |probe_sonicator| %>
      <tr>
        <td><%= probe_sonicator.sample %></td>
        <td><%= probe_sonicator.size %></td>
        <td><%= probe_sonicator.amplitude %></td>
        <td><%= probe_sonicator.volume %></td>
        <td><%= probe_sonicator.viscosity %></td>
        <td><%= probe_sonicator.more %></td>
        <td><%= probe_sonicator.debit %></td>
        <td><%= probe_sonicator.slotdate %></td>
        <td><%= probe_sonicator.slottime %></td>
        <td><%= probe_sonicator.status %></td>
        <td><%= link_to 'Show', probe_sonicator %></td>
        <td><%= link_to 'Edit', edit_probe_sonicator_path(probe_sonicator) %></td>
        <td><%= link_to 'Destroy', probe_sonicator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Probe Sonicator', new_probe_sonicator_path %>
