<p id="notice"><%= notice %></p>

<h1>Ultra Centrifuges</h1>

<table>
  <thead>
    <tr>
      <th>Sample</th>
      <th>Volume</th>
      <th>Speed</th>
      <th>Temperature</th>
      <th>Toxicity</th>
      <th>Compatibility</th>
      <th>Carcinogenic</th>
      <th>More</th>
      <th>Debit</th>
      <th>Slotdate</th>
      <th>Slottime</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ultra_centrifuges.each do |ultra_centrifuge| %>
      <tr>
        <td><%= ultra_centrifuge.sample %></td>
        <td><%= ultra_centrifuge.volume %></td>
        <td><%= ultra_centrifuge.speed %></td>
        <td><%= ultra_centrifuge.temperature %></td>
        <td><%= ultra_centrifuge.toxicity %></td>
        <td><%= ultra_centrifuge.compatibility %></td>
        <td><%= ultra_centrifuge.carcinogenic %></td>
        <td><%= ultra_centrifuge.more %></td>
        <td><%= ultra_centrifuge.debit %></td>
        <td><%= ultra_centrifuge.slotdate %></td>
        <td><%= ultra_centrifuge.slottime %></td>
        <td><%= ultra_centrifuge.status %></td>
        <td><%= link_to 'Show', ultra_centrifuge %></td>
        <td><%= link_to 'Edit', edit_ultra_centrifuge_path(ultra_centrifuge) %></td>
        <td><%= link_to 'Destroy', ultra_centrifuge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ultra Centrifuge', new_ultra_centrifuge_path %>
